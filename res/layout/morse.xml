<Keyboard
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:keyWidth="10%p"
  android:horizontalGap="0px"
  android:verticalGap="0px"
  android:keyHeight="@dimen/key_height">
  <Row android:keyHeight="@dimen/morse_keys_height">
    <Key android:codes="1" android:keyLabel="\u00B7" android:keyWidth="50%p" android:keyEdgeFlags="left" />
    <Key android:codes="2" android:keyLabel="\u2212" android:keyWidth="50%p" android:keyEdgeFlags="right" />
  </Row>
  <Row android:rowEdgeFlags="bottom">
    <Key
      android:codes="-1"
      android:keyIcon="@drawable/sym_keyboard_shift"
      android:keyWidth="20%p"
      android:isModifier="true"
      android:isSticky="true" />
    <Key
      android:codes="32"
      android:keyIcon="@drawable/sym_keyboard_space"
      android:keyWidth="60%p" />
    <Key
      android:codes="-5"
      android:isRepeatable="true"
      android:keyIcon="@drawable/sym_keyboard_delete"
      android:keyWidth="20%p" />
  </Row>
</Keyboard>
